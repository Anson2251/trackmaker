    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-legacy": "^6.0.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "naive-ui": "^2.41.0",
    "terser": "^5.39.0",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "~5.7.3",
