    "@tauri-apps/plugin-notification": "^2.2.1",
    "gcoord": "^1.0.7",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^5.1.0",
    "pinia": "^3.0.1",
    "ua-parser-js": "^2.0.2",
    "uuid": "^11.0.5",
    "vue": "^3.5.13",
    "vue-markdown-render": "^2.2.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.5",
