{
  "name": "trackmaker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "export RELEASE_MODE=false; vite dev",
    "serve": "export RELEASE_MODE=false; vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "build-release": "export RELEASE_MODE=true; npm run build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "build-preview": "npm run build-release; cd ./dist; python3 -m http.server 5173",
    "generate-docs": "typedoc"
  },
  "dependencies": {
    "gcoord": "^1.0.6",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^4.5.0",
    "pinia": "^2.1.7",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.5",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^9.0.8",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "naive-ui": "^2.38.1",
    "terser": "^5.31.3",
    "typedoc-plugin-markdown": "^4.0.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.11"
  }
}
